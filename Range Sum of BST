class Solution(object):
    def rangeSumBST(self, root, L, R):
        """
        :type root: TreeNode
        :type L: int
        :type R: int
        :rtype: int
        """
        self.sm=0
        def dfs(n):
            if n is None:
                return 0
            if n.val>=L and n.val<=R:
                self.sm+=n.val
            dfs(n.left)
            dfs(n.right)
            
            
        dfs(root)        
        return self.sm
        
